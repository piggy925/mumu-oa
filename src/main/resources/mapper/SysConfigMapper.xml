<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mumuoa.db.dao.SysConfigMapper">
    <select id="selectAllParam" resultType="com.example.mumuoa.db.pojo.SysConfig">
        SELECT param_key, param_value
        FROM sys_config
        WHERE status = 1
    </select>
</mapper>